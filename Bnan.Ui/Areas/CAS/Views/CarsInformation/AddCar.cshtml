                               data-val-required="@localizer["requiredFiled"]" />
                        <div style="margin-top:2px">
                            <div style="height: 1.35em">
                                @Html.ValidationMessage("BoardNumber", null, new { @class = "text-danger" })
                            </div>
                        </div>
                    </div>

