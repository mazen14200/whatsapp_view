                            {
                                @foreach (var MainColor in (List<SelectListItem>)ViewData["ColorsEn"])
                                {
                                    <option value="@MainColor.Value" selected="@(MainColor.Value.Trim() == "" ? "selected" : null)">@MainColor.Text</option>
                                }
